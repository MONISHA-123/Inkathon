{
  "welcomeFile": "/Essentiaries/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/Essentiaries/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
       {
            "source": "^/AdminModule",
            "target": "/",
            "destination": "AdminModule"
        }
  ]
}