<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.ink.Essentiaries.controller.About"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App id="essentiaries">
		<ScrollContainer horizontal="false" vertical="true" height="100%">
			<IconTabBar id="IconTabBar" select="handleIconTabBarSelect" class="sapUiResponsiveContentPadding" stretchContentHeight="true">
				<items>
					<IconTabFilter text="About Us" key="About Us">
						<ScrollContainer height="100%" width="100%" horizontal="false" vertical="true" class="Aboutus">
									<Image src="./images/aboutus.png" width="100%" height="auto"></Image>
						</ScrollContainer>
					</IconTabFilter>
					<IconTabSeparator/>
					<IconTabFilter text="FAQ" key="FAQ">
						<ScrollContainer height="100%" width="100%" horizontal="false" vertical="true"></ScrollContainer>
					</IconTabFilter>
					<IconTabSeparator/>
					<IconTabFilter text="Privacy Policy" key="Privacy Policy">
						<ScrollContainer height="100%" width="100%" horizontal="false" vertical="true" >
								<Image src="./images/comingSoon.jpg" width="100%" height="auto"></Image>
						</ScrollContainer>
					</IconTabFilter>
					<IconTabSeparator/>
					<IconTabFilter text="Careers" key="Careers">
						<ScrollContainer height="100%" width="100%" horizontal="false" vertical="true" >
								<Image src="./images/comingSoon.jpg" width="100%" height="auto"></Image>
						</ScrollContainer>
					</IconTabFilter>
					<IconTabSeparator/>
					<IconTabFilter key="Terms and Conditions" text="Terms and Conditions "> 
						<ScrollContainer height="100%" width="100%" horizontal="false" vertical="true">
							<Image src="./images/comingSoon.jpg" width="100%" height="auto"></Image>
						</ScrollContainer>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</ScrollContainer>
	</App>
</mvc:View>