
    <core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
    <Dialog contentWidth="40%">
        <customHeader>
            <Bar>
                <contentMiddle>
                    <Title text="{i18n>ShopName}"/>
                </contentMiddle>
                <contentRight>
                    <Button class="ToolbarIcon" icon="sap-icon://decline" tooltip="Close" press="fnToHome" type="Transparent"/>
                </contentRight>
            </Bar>
        </customHeader>
        <VBox fitContainer="true" justifyContent="Center" alignItems="Center" alignContent="Center">
        <Text text="Order Placed Successfully!!!"/>
       <!-- <Text text="Your Order Id is {oProductModel>/UserMasterOrder/orderid}"/>-->
        <Button text="Continue Shopping" press="fnToHome"/>
        </VBox>
            </Dialog>
</core:FragmentDefinition>
